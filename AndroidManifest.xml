<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gram.meme.journalapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivitiesAndFragments.DashBoardActvity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivitiesAndFragments.AddJournalActivity"
            android:theme="@style/AddJournalTheme" />
        <activity
            android:name=".ActivitiesAndFragments.JournalDetail"
            android:label="@string/title_activity_journal_detail"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>